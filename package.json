{
  "name": "mindless",
  "version": "0.0.1",
  "description": "Web app using express, mongodb, socketio, p5js",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"cross-env NODE_ENV=development nodemon .\" \"npm start --prefix client\"",
    "production": "npm run build --prefix client && node ."
  },
