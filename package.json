{
  "name": "mindless",
  "version": "0.0.1",
  "description": "Web app using express, mongodb, socketio, p5js",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"cross-env NODE_ENV=development nodemon .\" \"webpack --mode=development --config webpack.config.js\"",
    "production": "webpack --mode=production --config webpack.config.js && node ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/morswin22/mindless.git"
  },
  "author": "Patryk Janiak <xmorswinx@gmail.com> (https://stacket.dev/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/morswin22/mindless/issues"
  },
  "homepage": "https://github.com/morswin22/mindless#readme",
  "dependencies": {
    "concurrently": "^5.1.0",
    "cross-env": "^7.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.2",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10"
  },
  "nodemonConfig": {
    "ignore": [
      "build/*",
      "src/*"
    ]
  }
}
